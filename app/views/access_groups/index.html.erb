<nav class="navbar">
  <%= link_to t("common.back"), @map, class: "button button--link" %>
</nav>
<div class="container">
  <div class="section">
    <h2 class="section__title"><%= AccessGroup.model_name.human(count: 2) %></h2>
    <ul>
      <%= render @access_groups %>

      <%= render AccessGroup.new(map: @map, users: [User.new]) %>

      <%= render AccessGroup.new(map: @map, token: Devise.friendly_token.first(16)) %>
    </ul>
  </div>
</div>
